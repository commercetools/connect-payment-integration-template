{
  "name": "payment-integration-template",
  "version": "3.0.2",
  "description": "Payment provider integration template",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "node_modules/.bin/nodemon -q dist/main.js",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint --ext .ts src test",
    "lint:fix": "prettier --write \"**/**/*.{ts,js,json}\" && eslint --fix --ext .ts src test",
    "build": "rm -rf /dist && tsc",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"",
    "test": "jest --detectOpenHandles",
    "connector:post-deploy": "node src/connectors/post-deploy.ts",
    "connector:pre-undeploy": "node src/connectors/pre-undeploy.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commercetools-backend/loggers": "22.28.0",
    "@commercetools/connect-payments-sdk": "0.8.0",
    "@fastify/autoload": "5.9.0",
    "@fastify/cors": "9.0.1",
    "@fastify/formbody": "7.4.0",
    "@fastify/http-proxy": "9.5.0",
    "@fastify/request-context": "5.1.0",
    "@fastify/static": "7.0.4",
    "@fastify/type-provider-typebox": "4.0.0",
    "@sinclair/typebox": "0.32.32",
    "dotenv": "16.4.5",
    "fastify": "4.28.0",
    "fastify-plugin": "4.5.1"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-unused-imports": "3.2.0",
    "jest": "29.7.0",
    "msw": "2.3.1",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.3",
    "prettier": "3.3.2",
    "ts-jest": "29.1.4",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}