{
  "name": "payment-integration-template",
  "version": "4.6.2",
  "description": "Payment provider integration template",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "node_modules/.bin/nodemon -q dist/main.js",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint src test",
    "lint:fix": "prettier --write \"**/**/*.{ts,js,json}\" && eslint --fix src test",
    "build": "rm -rf /dist && tsc",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "connector:post-deploy": "node dist/connectors/post-deploy.js",
    "connector:pre-undeploy": "node dist/connectors/pre-undeploy.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commercetools-backend/loggers": "24.9.0",
    "@commercetools/connect-payments-sdk": "0.24.0",
    "@fastify/autoload": "6.3.1",
    "@fastify/cors": "11.1.0",
    "@fastify/formbody": "8.0.2",
    "@fastify/http-proxy": "11.3.0",
    "@fastify/request-context": "6.2.1",
    "@fastify/static": "8.3.0",
    "@fastify/type-provider-typebox": "5.2.0",
    "@sinclair/typebox": "0.33.22",
    "dotenv": "17.2.3",
    "fastify": "5.6.1",
    "fastify-plugin": "5.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@jest/globals": "30.2.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.9.0",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-unused-imports": "4.3.0",
    "globals": "^16.4.0",
    "jest": "30.2.0",
    "msw": "2.11.6",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "overrides": {
    "path-to-regexp": "3.3.0"
  }
}
