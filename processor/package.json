{
  "name": "payment-integration-template",
  "version": "4.2.0",
  "description": "Payment provider integration template",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "node_modules/.bin/nodemon -q dist/main.js",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint src test",
    "lint:fix": "prettier --write \"**/**/*.{ts,js,json}\" && eslint --fix src test",
    "build": "rm -rf /dist && tsc",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "connector:post-deploy": "node dist/connectors/post-deploy.js",
    "connector:pre-undeploy": "node dist/connectors/pre-undeploy.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commercetools-backend/loggers": "24.1.0",
    "@commercetools/connect-payments-sdk": "0.23.0",
    "@fastify/autoload": "6.3.1",
    "@fastify/cors": "11.0.1",
    "@fastify/formbody": "8.0.2",
    "@fastify/http-proxy": "11.1.2",
    "@fastify/request-context": "6.2.0",
    "@fastify/static": "8.2.0",
    "@fastify/type-provider-typebox": "5.1.0",
    "@sinclair/typebox": "0.33.22",
    "dotenv": "16.5.0",
    "fastify": "5.3.3",
    "fastify-plugin": "5.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@jest/globals": "30.0.3",
    "@types/jest": "29.5.14",
    "@types/node": "24.0.0",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jest": "28.13.0",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "^16.2.0",
    "jest": "29.7.0",
    "msw": "2.10.2",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "overrides": {
    "path-to-regexp": "3.3.0"
  }
}
