{
  "name": "payment-integration-template",
  "version": "4.1.0",
  "description": "Payment provider integration template",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "node_modules/.bin/nodemon -q dist/main.js",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint src test",
    "lint:fix": "prettier --write \"**/**/*.{ts,js,json}\" && eslint --fix src test",
    "build": "rm -rf /dist && tsc",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "connector:post-deploy": "node dist/connectors/post-deploy.js",
    "connector:pre-undeploy": "node dist/connectors/pre-undeploy.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commercetools-backend/loggers": "23.1.0",
    "@commercetools/connect-payments-sdk": "0.19.0",
    "@fastify/autoload": "6.1.0",
    "@fastify/cors": "10.0.2",
    "@fastify/formbody": "8.0.2",
    "@fastify/http-proxy": "11.1.2",
    "@fastify/request-context": "6.1.0",
    "@fastify/static": "8.1.1",
    "@fastify/type-provider-typebox": "5.1.0",
    "@sinclair/typebox": "0.33.22",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "fastify-plugin": "5.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.9",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "^16.0.0",
    "jest": "29.7.0",
    "msw": "2.7.3",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.9",
    "prettier": "3.5.3",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  },
  "overrides": {
    "path-to-regexp": "3.3.0"
  }
}
